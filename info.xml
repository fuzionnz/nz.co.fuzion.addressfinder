<?xml version="1.0"?>
<extension key="nz.co.fuzion.addressfinder" type="module">
  <file>addressfinder</file>
  <name>Address Finder</name>
  <description>Integrates Address Finder autocompletion into CiviCRM address fields.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Fuzion</author>
    <email>admin@fuzion.co.nz</author>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/addressfinder</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/addressfinder#</url>
    <url desc="Support">https://fuzion.co.nz</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-10-30</releaseDate>
  <version>1.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.66</ver>
  </compatibility>
  <comments>This loads on any page CiviCRM runs on this could cause problems!</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Addressfinder</namespace>
    <format>23.02.1</format>
    <angularModule>crmAddressfinder</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
